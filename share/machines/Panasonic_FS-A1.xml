<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>

  <info>
    <manufacturer>Panasonic</manufacturer>
    <code>FS-A1</code>
    <release_year>1986</release_year>
    <description/>
    <type>MSX2</type>
    <region>jp</region>
  </info>

  <CassettePort/>

  <devices>

    <S1985 id="S1985"> <!-- includes 5 bits mapper-read-back, as confirmed by Peter Hanraets with serial 6LAKC38645 -->
      <sramname>fs-a1_s1985.sram</sramname>
    </S1985>

    <PPI id="ppi">
      <sound>
        <volume>16000</volume>
      </sound>
      <io base="0xA8" num="4"/>
      <keyboard_type>jp_ansi</keyboard_type>
      <has_keypad>false</has_keypad>
      <key_ghosting_sgc_protected>false</key_ghosting_sgc_protected>
      <code_kana_locks>true</code_kana_locks>
      <graph_locks>false</graph_locks>
    </PPI>

    <VDP id="VDP">
      <version>V9938</version>
      <vram>128</vram>
      <io base="0x98" num="4" type="O"/>
      <io base="0x98" num="2" type="I"/>
    </VDP>

    <PSG id="PSG">
      <keyboardlayout>50on</keyboardlayout>
      <sound>
        <volume>21000</volume>
      </sound>
      <io base="0xA0" num="2" type="O"/>
      <io base="0xA2" num="1" type="I"/>
      <ignorePortDirections>true</ignorePortDirections> <!-- not sure, but guess based on use of Engine IC -->
    </PSG>

    <RTC id="Real time clock">
      <sramname>fs-a1.cmos</sramname>
      <io base="0xB4" num="2" type="O"/>
      <io base="0xB5" num="1" type="I"/>
    </RTC>

    <PrinterPort id="Printer Port">
      <io base="0x90" num="2"/>
    </PrinterPort>

    <primary slot="0">
      <ROM id="MSX BIOS with BASIC ROM">
        <rom> <!-- maskROM dump by FRS -->
          <filename>Panasonic_FS-A1_firmware.rom</filename>
          <sha1>7bbe3f355d3129592268ae87f40ea7e3ced88f98</sha1>
          <window base="0x0000" size="0x8000"/>
        </rom>
        <rom>
          <filename>fs-a1_basic-bios2.rom</filename>
          <sha1>0081ea0d25bc5cd8d70b60ad8cfdc7307812c0fd</sha1>
        </rom>
        <mem base="0x0000" size="0x8000"/>
      </ROM>
    </primary>

    <primary external="true" slot="1"/>

    <primary external="true" slot="2"/>

    <primary slot="3">

      <secondary slot="0">
        <RAM id="Main RAM">
          <mem base="0x0000" size="0x10000"/>
          <initialContent encoding="gz-base64"></initialContent>
          <!-- pattern:
               let a = (chr(  0)*16 + chr(255)*16)*2
               let b = (chr(255)*16 + chr(  0)*16)*2
               let X = a + b + a + b
               let Y = b + a + b + a
               let m00 = (X+Y)*3 + (X+X)*3 + (Y+X)*2
               let m01 = (X+Y)*3 + (X+X)*2 + (Y+X)*3
               let m03 = (X+Y)*2 + (X+X)*3 + (Y+X)*3
               let m10 = (Y+X)*3 + (Y+Y)*3 + (X+Y)*2
               let m11 = (Y+X)*3 + (Y+Y)*2 + (X+Y)*3
               let m12 = (Y+X)*2 + (Y+Y)*3 + (X+Y)*3
               let m13 = (Y+X)*2 + (Y+Y)*3 + (X+Y)*3
               let m20 = (Y+X)*3 + (Y+Y)*3 + (X+Y)*2
               let m21 = (Y+X)*3 + (Y+Y)*3 + (X+Y)*2
               let m22 = (Y+X)*3 + (Y+Y)*2 + (X+Y)*3
               let m23 = (Y+X)*2 + (Y+Y)*3 + (X+Y)*3
               let m30 = (X+Y)*3 + (X+X)*3 + (Y+X)*2
               let m31 = (X+Y)*3 + (X+X)*3 + (Y+X)*2
               let m32 = (X+Y)*3 + (X+X)*2 + (Y+X)*3
               let m33 = (X+Y)*2 + (X+X)*3 + (Y+X)*3
               let m0 = m00 + m01 + m02 + m03
               let m1 = m10 + m11 + m12 + m13
               let m2 = m20 + m21 + m22 + m23
               let m3 = m30 + m31 + m32 + m33
               content = (m0+m1+m2+m3).encode('zip').encode('base64')
               As found the machine with serial 6LAKC38645.
               -->
        </RAM>
      </secondary>

      <secondary slot="1">
        <ROM id="MSX Sub ROM">
          <rom> <!-- maskROM dump by FRS -->
            <filename>Panasonic_FS-A1_firmware.rom</filename>
            <sha1>7bbe3f355d3129592268ae87f40ea7e3ced88f98</sha1>
            <window base="0x8000" size="0x4000"/> <!-- NOTE: actually, it should be size = 0x8000, but that 2nd block is all 0xFF and that is mapped in there by default already. So, to be backwards compatible with the separate subROM dump, I only use 0x4000 here as size -->
          </rom>
          <rom>
            <filename>fs-a1_msx2sub.rom</filename>
            <sha1>0fbd45ef3dd7bb82d4c31f1947884f411f1ca344</sha1>
          </rom>
          <mem base="0x0000" size="0x4000"/>
        </ROM>
      </secondary>

      <secondary slot="2">
        <ROM id="Desk Pac 1">
          <rom> <!-- maskROM dump by FRS -->
            <filename>Panasonic_FS-A1_firmware.rom</filename>
            <sha1>7bbe3f355d3129592268ae87f40ea7e3ced88f98</sha1>
            <window base="0x10000" size="0x8000"/>
          </rom>
          <rom>
            <filename>fs-a1_deskpac1.rom</filename>
            <sha1>63098f27beac9eca6b39d837d2a552395df33fe1</sha1>
          </rom>
          <mem base="0x4000" size="0x8000"/>
        </ROM>
      </secondary>

      <secondary slot="3">
        <ROM id="Desk Pac 2">
          <rom> <!-- maskROM dump by FRS -->
            <filename>Panasonic_FS-A1_firmware.rom</filename>
            <sha1>7bbe3f355d3129592268ae87f40ea7e3ced88f98</sha1>
            <window base="0x18000" size="0x8000"/>
          </rom>
          <rom>
            <filename>fs-a1_deskpac2.rom</filename>
            <sha1>7f5b76605e3d898cc4b5aacf1d7682b82fe84353</sha1>
          </rom>
          <mem base="0x4000" size="0x8000"/>
        </ROM>
      </secondary>

    </primary>

  </devices>

</msxconfig>
