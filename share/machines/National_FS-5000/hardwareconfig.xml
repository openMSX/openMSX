<?xml version="1.0" ?>
<!-- $Id$ -->
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>

  <info>
    <manufacturer>National</manufacturer>
    <code>FS-5000</code>
    <release_year>1986</release_year>
    <description/>
    <type>MSX2</type>
  </info>

  <MapperReadBackBits>largest</MapperReadBackBits>

  <CassettePort/>

  <devices>

    <Kanji id="Kanji ROM">
      <rom>
        <filename>roms/fs-5000_kanjifont.rom</filename>
        <sha1>5e872d5853698731a0ed22fb72dbcdfd59cd19c3</sha1>
      </rom>
      <io base="0xD8" num="2" type="O"/>
      <io base="0xD9" num="1" type="I"/>
    </Kanji>

    <S1985 id="S1985"/>

    <PPI id="ppi">
      <key_ghosting>true</key_ghosting>
      <sound>
        <volume>16000</volume>
      </sound>
      <io base="0xA8" num="4"/>
    </PPI>

    <VDP id="VDP">
      <version>V9938</version>
      <vram>128</vram>
      <io base="0x98" num="4" type="O"/>
      <io base="0x98" num="2" type="I"/>
    </VDP>

    <PSG id="PSG">
      <keyboardlayout>JIS</keyboardlayout>
      <sound>
        <volume>21000</volume>
      </sound>
      <io base="0xA0" num="2" type="O"/>
      <io base="0xA2" num="1" type="I"/>
    </PSG>

    <RTC id="Real time clock">
      <sramname>fs-5000.cmos</sramname>
      <io base="0xB4" num="2" type="O"/>
      <io base="0xB5" num="1" type="I"/>
    </RTC>

    <PrinterPort id="Printer Port">
      <io base="0x90" num="2"/>
    </PrinterPort>

    <primary slot="0">

      <secondary slot="0">
        <ROM id="MSX BIOS with BASIC ROM">
          <rom>
            <filename>roms/fs-5000_basic-bios2.rom</filename>
            <sha1>59967765d6e9328909dee4dac1cbe4cf9d47d315</sha1>
          </rom>
          <mem base="0x0000" size="0x8000"/>
        </ROM>
      </secondary>

      <secondary slot="1"/>

      <secondary slot="2"/>

      <secondary slot="3"/>

    </primary>

    <primary external="true" slot="1"/>

    <primary external="true" slot="2"/>

    <primary slot="3">

      <secondary slot="0">
        <ROM id="MSX Sub ROM">
          <rom>
            <filename>roms/fs-5000_msx2sub.rom</filename>
            <sha1>0fbd45ef3dd7bb82d4c31f1947884f411f1ca344</sha1>
          </rom>
          <mem base="0x0000" size="0x4000"/>
        </ROM>
        <ROM id="MSX Kanji Driver">
          <rom>
            <filename>roms/fs-5000_kanjibasic.rom</filename>
            <sha1>3a9a942ed888dd641cddf8deada1879c454df3c6</sha1>
          </rom>
          <mem base="0x4000" size="0x8000"/>
        </ROM>
      </secondary>

      <secondary slot="1">
        <ROM id="Set-Up RTC">
          <rom>
            <filename>roms/fs-5000_setuprtc.rom</filename>
            <sha1>98bbfa3ab07b7a5cad55d7ddf7cbd9440caa2a86</sha1>
          </rom>
          <mem base="0x4000" size="0x8000"/>
        </ROM>
      </secondary>

      <secondary slot="2">
        <MemoryMapper id="Main RAM">
          <size>128</size>
          <mem base="0x0000" size="0x10000"/>
        </MemoryMapper>
      </secondary>

      <secondary slot="3">
        <MB8877A id="Memory Mapped FDC">
          <brokenFDCread>false</brokenFDCread>
          <drives>2</drives>
          <rom>
            <filename>roms/fs-5000_disk.rom</filename>
            <sha1>073feb8bb645d935e099afaf61e6f04f52adee42</sha1>
          </rom>
          <mem base="0x4000" size="0x8000"/>
        </MB8877A>
      </secondary>

    </primary>

  </devices>

</msxconfig>
