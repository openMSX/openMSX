<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>
  <info>
    <name>Carnivore2</name>
    <manufacturer>RBSC</manufacturer>
    <code/>
    <release_year>2017</release_year>
    <description>Carnivore2 is a multi-device MSX cartridge created by the Russian Bear Service Crew in 2017 that incorporates 1Mb of RAM with mapper, 1Mb of shadow RAM (with FMPAC SRAM emulation), IDE controller with Nextor BIOS, emulated FMPAC, SCC/SCC+ and PSG devices as well as 8Mb FlashROM. The cartridge uses CF cards as IDE media.</description>
    <type>Flash cartridge</type>
  </info>
  <devices>
    <primary slot="any">
      <Carnivore2 id="Carnivore2">
        <mem base="0x0000" size="0x10000"/>
        <io base="0x3C" num="1" type="O"/>
        <io base="0x7C" num="2" type="O"/>
        <rom>
          <sha1>1ad39f7bfa384b8940073bf0c5ad0bff95aee004</sha1>
        </rom>
        <sramname>carnivore2.flash</sramname>
        <eeprom>
          <sramname>carnivore2.eeprom</sramname>
        </eeprom>
        <sound>
          <volume>21000</volume>
        </sound>
        <master>
          <type>IDEHD</type>
          <filename>hd.dsk</filename>
          <size>100</size>
        </master>
        <!--<slave></slave>-->
      </Carnivore2>
    </primary>
  </devices>
</msxconfig>
