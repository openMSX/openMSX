<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>
  <info>
    <name>MegaRAM (anniversary ed.)</name>
    <manufacturer>Ademir Carchano</manufacturer>
    <code/>
    <release_year>2014</release_year>
    <description>2MB MegaRAM: 27th anniversary of MegaRAM edition.</description>
    <type>RAM expansion</type>
  </info>
  <devices>
    <primary slot="any">
      <secondary slot="any">
        <MegaRam id="MegaRam">
          <io base="0x8e" num="1"/>
          <mem base="0x0000" size="0x10000"/>
          <size>2048</size>
        </MegaRam>
      </secondary>
    </primary>
  </devices>
</msxconfig>
